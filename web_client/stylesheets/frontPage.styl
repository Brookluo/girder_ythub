.g-frontpage-header
  padding 5px

  .g-frontpage-title-container
    display inline-block
    margin-left 12px

    .g-frontpage-title
      font-weight bold
      font-size 34px
      vertical-align top
      color #222
  
    .g-frontpage-sublogo
      display inline-block
      vertical-align top
      margin-left 18px
      margin-top -7px

  .g-frontpage-logo
    display inline-block
    vertical-align top
    margin-left 2px

.g-frontpage-body
  padding 12px 12px 0 12px
  margin-top 10px

  border-radius 2px
  background-color #f8f8f8
  border 1px solid #e5e5e5
  color #555

  .g-frontpage-paragraph
    margin-top 0
    margin-bottom 18px
    font-size 14px

  .g-frontpage-sponsor
    margin-left 15px
